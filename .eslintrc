{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 9,
    "ecmaFeatures": {
      "jsx": true
    },
    "sourceType": "module"
  },
  "rules": {
    "no-underscore-dangle": "off",
    "no-param-reassign": 0,
    "no-unused-vars": "off",
    "no-nested-ternary": "off",
    "no-console": "off",
    "max-len": "off",
    "comma-dangle": "off",
    "quotes": [2, "double"],
    "no-alert": "off",
    "prefer-arrow-callback": "off",
    "import/no-extraneous-dependencies": "off",
    "func-names": "off",
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/prefer-stateless-function": 0,
    "arrow-parens": [2, "always"],
    "jsx-a11y/label-has-associated-control": 0
  },
  "env": {
    "browser": true,
    "node": true,
    "jest": true
  }
}
